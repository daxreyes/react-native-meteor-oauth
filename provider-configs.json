{
  "Facebook": {
    "url": "https://www.facebook.com/v2.2/dialog/oauth",
    "extraRequestParams": {
      "display": "popup"
    },
    "scope": "profile"
  },
  "Github": {
    "url": "https://github.com/login/oauth/authorize",
    "scope": "repo" 
  },
  "Google": {
    "extraRequestParams": {
      "response_type": "code"
    },
    "url": "https://accounts.google.com/o/oauth2/v2/auth",
    "scope": "email"
  }
}
